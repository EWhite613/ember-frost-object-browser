{{#frost-object-browser}}
  {{#block-slot 'info-bar'}}
    {{#frost-info-bar}}
      {{#block-slot 'title'}}
        Resources
      {{/block-slot}}
      {{#block-slot 'summary'}}
        20 â€“ L3VPN Services
      {{/block-slot}}
      {{#block-slot 'actions' as |controls|}}
        {{controls.button
          icon='frost/infobar-create'
          onClick=(action 'onCreate')
          text='Create'
        }}
      {{/block-slot}}
    {{/frost-info-bar}}
  {{/block-slot}}
  {{#block-slot 'facets'}}
    {{frost-object-browser-filter filters=filters onFilter=onOptionSelected}}
  {{/block-slot}}
  {{#block-slot 'view' as |onSelect selections|}}
   {{demo/content-view
    itemsPerPage=6
    model=model.model
    onDetailChange=(action "onDetailChange")
    onRowSelect=onSelect
    values=model.resources
    viewSchema=viewSchema
   }}
  {{/block-slot}}
  {{#block-slot 'actions' as |controls selections|}}
    {{controls.button
      onClick=(action 'delete')
      multiSelect=true
      priority="secondary"
      text='Delete'
    }}
    {{controls.button
      onClick=(action 'edit')
      priority="secondary"
      text='Edit'
    }}
    {{#controls.link 'details'
      multiSelect=true
      priority='primary'
    }}
      Details
    {{/controls.link}}
  {{/block-slot}}
{{/frost-object-browser}}
